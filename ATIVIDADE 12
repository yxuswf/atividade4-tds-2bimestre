programa
{
    funcao inicio()
    {
        inteiro i, numero, maior

        escreva("Digite o 1º número: ")
        leia(numero)

        maior = numero

        para (i = 2; i <= 5; i++){
            escreva("Digite o ", i, "º número: ")
            leia(numero)

            se (numero > maior){
                maior = numero
            }
        }
        escreva("O maior número é: ", maior)
    }
}
