programa
{
    funcao inicio()
    {
        inteiro nota

        escreva("Informe uma nota entre 0 e 10: ")
        leia(nota)

        se(nota < 0 ou nota > 10){
        	escreva("Informe uma nota v√°lida!")
        }
        senao se(nota <= 10)
        	escreva("Agradecemos pela sua nota!")
    }
}
